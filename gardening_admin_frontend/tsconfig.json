{
  "files": [],
  "references": [
    { "path": "./tsconfig.app.json" },
    { "path": "./tsconfig.node.json" }
  ],
  "compilerOptions": {
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmitOnError": false,
    "isolatedModules": true,
    "baseUrl": "./src",
    "paths": {
      "@/*": ["./*"],
      "src/*": ["./*"]
    }
  }

}
